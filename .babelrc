{
      "presets": ["@babel/preset-env"],
      "plugins": [
            [
                  "babel-plugin-root-import",
                  {
                        "rootPathPrefix": "@/",
                        "rootPathSuffix": "./src/"
                  }
            ]
      ],
      "env": {
            "debug": {
                  "sourceMaps": "inline",
                  "retainLines": true
            }
      }
}
